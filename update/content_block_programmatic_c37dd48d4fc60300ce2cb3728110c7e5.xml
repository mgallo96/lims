        <sys_policy/>
        <sys_scope display_value="lims">ed5318484f210300ce2cb3728110c7fc</sys_scope>
        <sys_update_name>content_block_programmatic_c37dd48d4fc60300ce2cb3728110c7e5</sys_update_name>
        <sys_updated_by>admin</sys_updated_by>
        <sys_updated_on>2017-10-24 17:02:10</sys_updated_on>
        <two_phase>true</two_phase>
    </content_block_programmatic>
</record_update>
